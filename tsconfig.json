{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2017",
    "module": "ESNext",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node"
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}
